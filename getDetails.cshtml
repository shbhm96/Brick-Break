@model List<MVCApplication.Models.Car>
@using MVCApplication.Models

@{
    ViewBag.Title = "getDetails";
}
<table class="table table-hover">
    <tr>
        <th>
           Id     
        </th>
        <th>
            Category
        </th>
        <th>
            Name
        </th>
        <th>
            Region
        </th>
        <th>
            Color
        </th>
        <th>
            Price
        </th>
        </tr>

   @{ 
       foreach(Car c in Model)
       {
           <tr>
              
               <td>
                   @c.id
               </td>
         
               <td>
                   @c.category
               </td>
          
            <td>
                @c.Name
            </td>
     
            <td>
                @c.region
            </td>
       
            <td>
                @c.color
            </td>

               <td>
                   @c.price
               </td>
             <td>
                 <button type="button" class="btn btn-success"><b>Buy</b></button>
             </td>
        </tr>
       }

}
</table>

